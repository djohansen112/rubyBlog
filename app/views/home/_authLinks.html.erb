<%= stylesheet_link_tag "authLinks" %>
<div>
  <%# if authenticated %>
  <% if true %>
    <%= link_to "Login", home_homepage_path, class:"navbarlink"%>
  <% elsif %>
    <div>
      <% link_to "New Post", new_blog_post_path, class:"authlink"%>
      <% link_to "Log out", destroy_user_session_path, class:"authlink"%>
    </div>
  <% end %>
  <div class="authburger">
    <div class="authline"></div>
    <div class="authline"></div>
    <div class="authline"></div>
  </div>
  <%# if menu is open beacuse of the CSS size of screen %>
  <% if false %>
    <div class="authresponsiveMenu">
      <%= link_to "Home", home_homepage_path, class: "authlink"%>
      <%= link_to "Contact", home_contact_path, class: "authlink"%>
      <%= link_to "About", home_about_path, class: "authlink"%>
      <%# if not authenticated %>
      <% if false %>
        <%= link_to "Login", home_homepage_path, class:"authlink"%>
      <% elsif %>
        <%= link_to "New Post", new_blog_post_path, class:"authlink"%>
        <%= link_to "Log out", destroy_user_session_path, class:"authlink"%>
      <% end %>
    </div>
  <% end %>
</div>
</div>
